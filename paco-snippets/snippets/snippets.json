{
    "Use strict": {
        "prefix": "ust",
        "body": [
            "'use strict';",
            "$1"
        ],
        "description": "Add use strict to the file"
    },
    "Arrow function": {
        "prefix": "afn",
        "body": [
            "($1) => {",
                "$3",
            "}$2"
        ],
        "description": "Create an arrow function"
    },
    "Require module": {
        "prefix": "adreq",
        "body": [
            "const $1 = require('$2');",
            "$3"
        ],
        "description": "Require a module"
    }
}